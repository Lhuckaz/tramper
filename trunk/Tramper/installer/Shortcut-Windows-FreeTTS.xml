<?xml version="1.0" encoding="iso-8859-1" standalone="yes" ?>
<shortcuts>

  <skipIfNotSupported/>
  <programGroup defaultName="Tramper" location="applications"/>
  <shortcut
    name="Tramper"
    target="$JAVA_HOME\bin\javaw.exe"
    commandLine="-Xmx512m -Djava.library.path=lib\windows\x86 -jar lib\Tramper.jar"
    workingDirectory="$INSTALL_PATH"
    description="This starts the free and portable version of Tramper"
    iconFile="$INSTALL_PATH\Tramper.ico"
    iconIndex="0"
    initialState="noShow"
    programGroup="yes"
    desktop="yes"
    applications="no"
    startMenu="yes"
    startup="no">
    <createForPack name="browser"/>
  </shortcut>
  
  <shortcut
    name="Uninstall Tramper"
    target="$JAVA_HOME\bin\javaw.exe"
    commandLine="-jar Uninstaller\uninstaller.jar"
    workingDirectory="$INSTALL_PATH"
    description="This uninstall Tramper"
    iconFile="$INSTALL_PATH\Tramper-uninstall.ico"
    iconIndex="0"
    initialState="noShow"
    programGroup="yes"
    desktop="no"
    applications="no"
    startMenu="no"
    startup="no">
    <createForPack name="browser"/>
  </shortcut>
  
</shortcuts>

