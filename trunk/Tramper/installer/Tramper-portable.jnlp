<?xml version="1.0" encoding="UTF-8"?>
<jnlp codebase="http://tramper.com/downloads/" href="Tramper-portable.jnlp">
    <information>
        <title>Tramper browser</title>
        <vendor>Paul-Emile Veuve</vendor>
        <description>Tramper is a web browser for the visually impaired people.</description>
        <icon href="Tramper.png"/>
        <icon href="Tramper.png" kind="splash"/>
        <shortcut online="false">
            <desktop/>
            <menu submenu="Tramper"/>
