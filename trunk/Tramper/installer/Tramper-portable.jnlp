        <related-content href="Readme.txt">
            <title>Instructions</title>
        </related-content>
        <association extensions="rss" mime-type="application/rss+xml"/>
        <association extensions="rdf" mime-type="application/rdf+xml"/>
        <association extensions="atom" mime-type="application/atom+xml"/>
        <association extensions="opml" mime-type="text/x-opml"/>
        <association extensions="html" mime-type="text/html"/>
        <offline-allowed />
    </information>
